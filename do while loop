import java.util.Scanner;
public class event{
    public static void main(String[]srgs)
    {
        int i=0;
        System.out.println("enter the number");
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       do
        {
            System.out.println(i);
            i=i+2;
        }
         while(i<=n);
    }
}
